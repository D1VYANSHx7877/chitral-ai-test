{
  "version": 2,
  "buildCommand": "npm run install:all && cd frontend && npm run build",
  "installCommand": "npm run install:all",
  "outputDirectory": "frontend/dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
