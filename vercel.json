{
  "version": 2,
  "buildCommand": "cd api && npm install && cd ../frontend && npm install && npm run build && cd ../backend && npm install",
  "outputDirectory": "frontend/dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
          {
      "source": "/api/auth/(.*)",
      "destination": "/api/auth.js"
    },
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
